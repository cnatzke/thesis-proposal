  journal  = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
  author   = {Aksoy, A. and Naqvi, A. A. and Khiari, F. Z. and Abujarad, F. and Al-Ohali, M. and Sumani, M.},
  month    = dec,
  year     = {1994},
  pages    = {558--561},
  file     = {ScienceDirect Full Text PDF:/Users/cnatzke/Zotero/storage/8UXP7TVL/Aksoy et al. - 1994 - Elemental analysis using natural gamma-ray spectro.pdf:application/pdf;ScienceDirect Snapshot:/Users/cnatzke/Zotero/storage/L2IXCEDX/0168900294917221.html:text/html}
}

