
@article{kramp_nuclear_1987,
  title    = {Nuclear two-photon decay in 0+→0+ transitions},
  volume   = {474},
  issn     = {03759474},
  doi      = {10.1016/0375-9474(87)90625-7},
  abstract = {The two-photon decay of the first excited 0+ state of 16O has been measured using the Heidelberg-Darmstadt crystal ball. A branching ratio of Γγγ Γtot = (6.6±0.5) · 10-4 was obtained. As in the cases of 40Ca and 90Zr previously reported by us, the 2γ decay of 16O proceeds via double E1 and M1 transitions of similar strength; the evidence is the observed interference term in the 2γ angular correlation. The ratio of the matrix elements αE1 χ for 16O was restricted to the two inverse values (-6.2±1.5) or (-0.16±0.04). An interpretation of 2γ matrix elements observed for 16O, 40Ca and 90Zr in terms of the electric polarizabilities and magnetic susceptibility is given leading to a qualitative understanding of this decay mode. © 1987.},
  number   = {2},
  journal  = {Nuclear Physics, Section A},
  author   = {Kramp, J. and Habs, D. and Kroth, R. and Music, M. and Schirmer, J. and Schwalm, D. and Broude, C.},
  year     = {1987},
  keywords = {Nuclear reactions},
  pages    = {412--450},
  file     = {PDF:/Users/cnatzke/Zotero/storage/KRYQ8QIC/Kramp et al._1987_Nuclear two-photon decay in 0→0 transitions.pdf:application/pdf}
}

